{# templates/user/list.html.twig #}

{% extends 'base.html.twig' %}

{% block title %}User List{% endblock %}

{% block body %}
    <h2>User List</h2>
    <ul>
        {% for user in users %}
            <li>{{ user.email }}</li> {# Affiche le login (email) de l'utilisateur #}
        {% endfor %}
    </ul>
{% endblock %}
